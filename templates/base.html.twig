<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <!-- <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text><text y=%221.3em%22 x=%220.2em%22 font-size=%2276%22 fill=%22%23fff%22>sf</text></svg>"> -->
            <link rel="icon" href="{{ asset('img/glider.svg') }}">
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
            {% block importmap %}{{ importmap('app') }}{% endblock %}
        {% endblock %}
    </head>
    <body>
        <div class="wrapper">
            <nav class="navbar">
                <ul>
                    <a class="navb" href="{{ path('home') }}">Home</a>
                    <a class="navb" href="{{ path('about') }}">About</a>
                    <a class="navb" href="{{ path('report') }}">Report</a>
                    <a class="navb" href="{{ path('lucky') }}">Lucky</a>
                    <a class="navb" href="{{ path('api') }}">API</a>
                </ul>
            </nav>
            {% block body %}{% endblock %}
            <footer class="footer">
                <a href="https://github.com/balkanboj">Github!</a>
            </footer>
        </div>
    </body>
</html>
